# keep/delete old printer ppds
manuf="Tally Compaq KonicaMinolta Generic Okidata HP Minolta Gestetner NEC Fujifilm Xerox Ricoh Olympus Olivetti Citizen DEC Heidelberg IBM Seiko Epson SiPix Avery QMS Lexmark Dell Samsung Toshiba Sony PCPI Apollo Alps Infotec ESP Oce Brother Kodak Fujitsu Anitech Raven Star NRG Imagen Pentax Mitsubishi Apple Canon Savin Tektronix Hitachi Kyocera CItoh Zebra Sharp Panasonic Dymo Lanier Dymo-CoStar"

if [ $JS_LAST_EXECUTED_VERSION -le 4 ]; then
	if [ "$cups_keep_ppds" != "true" ]; then
		backupfile=/var/backups/ldap-hp-ppds-`date +"%d-%m-%y-%H-%M-%S"`
		ldapsearch -x -ZZ -D "$ldap_hostdn" -y /etc/machine.secret 'objectclass=univentionPrinterModels' >  $backupfile
		for i in $manuf; do
			ldapsearch -x -ZZ -D "$ldap_hostdn" -y /etc/machine.secret "(&(objectclass=univentionPrinterModels)(cn=$i))" dn | grep dn: >/dev/null
			if [ $? -eq 0 ]; then
				univention-directory-manager settings/printermodel remove "$@" --dn "cn=$i,cn=cups,cn=univention,$ldap_base"
			fi
		done
	fi
fi


univention-directory-manager container/cn create "$@" --ignore_exists --position "cn=univention,$ldap_base" --set name=cups

