oComp2Guid.add "c5.ALL.1.0.0.UCS_A.INSTALLDIR_1_lsremora.dll", "{E2024A5E-03D2-4A2C-A7F5-9CF7676C8F63}"
oComp2Guid.add "c8.ALL.1.0.0.UCS_A.INSTALLDIR_1_PwDump.exe", "{CA9BD315-F647-4D20-9F68-4A307122C6AB}"
