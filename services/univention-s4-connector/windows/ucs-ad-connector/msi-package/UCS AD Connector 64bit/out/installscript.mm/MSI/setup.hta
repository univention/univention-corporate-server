oComp2Guid.add "c11.ALL.1.0.0.UCS_A.INSTALLDIR_1_ssleay32.dll", "{3ECAF153-C755-4A3E-A831-A1AEB7E144E3}"
oComp2Guid.add "c4.ALL.1.0.0.UCS_A.INSTALLDIR_1_libssl32.dll", "{46F400EB-AFF6-4250-AB4E-8546CF914C9E}"
oComp2Guid.add "c12.ALL.1.0.0.UCS_A.INSTALLDIR_1_ucs_ad_connector.exe", "{60B4234D-0B9C-4D29-9102-21F4552015CD}"
