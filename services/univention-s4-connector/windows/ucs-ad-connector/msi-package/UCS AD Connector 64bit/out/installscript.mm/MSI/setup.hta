on error resume next
T = "<p>"
T = T & "<center>"
T = T & "  <span onclick='VBSCRIPT:HeadingExpandContract H1_4, HTMLRPT_HEADING_4' title='Scripting is disabled, sections can not be contracted or expanded!'> <CENTER><H1 id='H1_4' class='H1NoButton'>File Verification Report</H1></CENTER> </span> <div ID='HTMLRPT_HEADING_4' style='display:block'> <" & "script LANGUAGE='VBSCRIPT'> HeadingStateExpanded H1_4, HTMLRPT_HEADING_4 <" & "/script>"
HtmlOutput(T & GetInstalledState() & "<p>")

set oComp2Guid = CreateObject("Scripting.Dictionary")
oComp2Guid.add "c10.ALL.1.0.0.UCS_A.INSTALLDIR_1_servpw64.exe", "{4459788F-31E6-41AE-9139-AC2815C70BD3}"
oComp2Guid.add "c9.ALL.1.0.0.UCS_A.INSTALLDIR_1_servpw.exe", "{CC6E4A66-0B8E-4A35-92DD-A5A4B3595115}"
