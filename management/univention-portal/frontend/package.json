{
  "name": "upx-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "serve:wait": "yarn serve & wait-on http://localhost:8080",
    "deploy:firebase": "yarn build && firebase deploy",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:updatesnapshots": "yarn test:unit --updateSnapshot",
    "test:unit:watch": "yarn test:unit --watch",
    "test:e2e": "NODE_ENV=development vue-cli-service test:e2e",
    "test:e2e:headless": "NODE_ENV=development vue-cli-service test:e2e --headless",
    "test:e2e:run": "yarn cypress run",
    "lint": "vue-cli-service lint --no-fix",
    "lint:all": "yarn lint && stylelint \"{**/*.{scss,vue,styl}\"",
    "lint:fix": "vue-cli-service lint",
    "lint:fix:all": "yarn lint:fix && stylelint --fix \"{**/*.{scss,vue,styl}\""
  },
  "dependencies": {
    "axios": "^0.21.2",
    "core-js": "^3.6.5",
    "cypress-axe": "^0.13.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^3.0.11",
    "vue-dompurify-html": "^3.0.0-alpha.3",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "axe-core": "^4.3.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "<25.0.0",
    "cypress": "^6.3.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-wait-until": "^1.7.1",
    "eslint": "^7.21.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-vue": "^7.7.0",
    "jest-serializer-vue": "^2.0.2",
    "stylelint": "^13.9.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.9.3",
    "vue-eslint-parser": "^7.6.0",
    "vue-jest": "^5.0.0-0",
    "wait-on": "^5.3.0",
    "yarn": "^1.22.10"
  }
}
