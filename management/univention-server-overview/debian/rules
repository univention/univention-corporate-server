override_dh_auto_test:
	ucslint
	dh_auto_test

override_dh_auto_build:
	dh-umc-module-build
	univention-l10n-build de
	dh_auto_build

override_dh_auto_install:
	univention-install-config-registry
	dh-umc-module-install
	univention-l10n-install de
	dh_auto_install

%:
	dh $@ --with 'python2,python3'
